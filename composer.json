{
    "name": "dongrim/laravel-toastr",
    "description": "Toast notifications for laravel 5.5+, 6 and 7",
    "keywords": [
      "toastr",
      "laravel",
      "notification",
      "facade",
      "helper",
      "auto discover",
      "php"
    ],
    "homepage": "",
    "license": "MIT",
    "type": "package",
    "authors": [
      {
        "name": "Yaroslav Fedan",
        "email": "yaroslav.fd@gmail.com"
      }
    ],
    "require": {
      "php": ">=7.0"
    },
    "require-dev": {
      "illuminate/support": "^5.5.0",
      "illuminate/session": "^5.5.0"
    },
    "autoload": {
      "files": [
        "helpers/helpers.php"
      ],
      "psr-4": {
        "Dongrim\\Toastr\\":"src/"
      }
    },
    "minimum-stability": "stable",
    "extra": {
      "laravel": {
        "providers": [
          "Dongrim\\Toastr\\ToastrServiceProvider"
        ],
        "aliases": {
          "Toastr": "Dongrim\\Toastr\\Facades\\Toastr"
        }
      }
    }
  }
